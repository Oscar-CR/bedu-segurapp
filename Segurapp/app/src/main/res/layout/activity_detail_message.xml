<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.home.DetailContactActivity"
    >

    <TextView
        android:id="@+id/tvDetailMessageUser"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nombre Contactos"
        android:layout_centerInParent="true"
        android:textSize="@dimen/text_32"
        android:textStyle="bold"
        />

    <TextView
        android:id="@+id/tvDetailMessageText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tvDetailMessageUser"
        android:text="Mensaje"
        android:layout_centerInParent="true"
        android:textSize="@dimen/text_24"
        android:layout_marginHorizontal="@dimen/size_24"
        />



</RelativeLayout>